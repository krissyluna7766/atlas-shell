#!/usr/bin/env bash
# This is a Bash script that loops from 1 to 10 and displays messages based on iteration.
# Counter variable
count=1
# While loop to iterate from 1 to 10
while [ $count -le 10 ]; do
    if [ $count -eq 4 ]; then
        echo "bad luck"
    elif [ $count -eq 8 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi
    ((count++))
done
